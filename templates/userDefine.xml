      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="dep" value="&lt;dependency&gt;&#10;    &lt;groupId&gt;&lt;/groupId&gt;&#10;    &lt;artifactId&gt;$end$&lt;/artifactId&gt;&#10;    &lt;version&gt;&lt;/version&gt;&#10;&lt;/dependency&gt;" description="add a dependency for pom.xml" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>