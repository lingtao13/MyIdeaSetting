<keymap version="1" name="Default copy" parent="$default">
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt quote" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt semicolon" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
</keymap>