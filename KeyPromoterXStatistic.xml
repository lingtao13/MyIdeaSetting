<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'FirstMissingPo....main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Run 'FirstMissingPo....main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'LongestValidPa....main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Run 'LongestValidPa....main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Into">
        <Item>
          <IdeaActionID>StepInto</IdeaActionID>
          <ShortCut>'F7'</ShortCut>
          <Description>Step Into</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'LongestValidParentheses'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'LongestValidParentheses'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>